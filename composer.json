    "keywords": ["symfony", "easyadmin", "fields"],
    "homepage": "https://github.com/agence-adeliom/easy-fields-bundle",
    "support": {
        "issues": "https://github.com/agence-adeliom/easy-fields-bundle/issues",
        "source": "https://github.com/agence-adeliom/easy-fields-bundle"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Arnaud RITTI",
            "homepage": "https://github.com/arnaud-ritti"
        },
        {
